{
  "name": "famicons",
  "version": "5.1.2",
  "description": "Premium icons for Family.",
  "files": [
    "dist/",
    "icons/"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/famicons.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "scripts": {
    "build": "tsc -p scripts/tsconfig.json && npm run build.files && npm run build.component && npm run collection.copy",
    "build.files": "node scripts/build.js",
    "build.component": "rindo build",
    "collection.copy": "node scripts/collection-copy.js",
    "start": "rindo build --dev --watch --serve",
    "release.prepare": "node scripts/release.js --any-branch",
    "release": "node scripts/release.js --publish --any-branch"
  },
  "devDependencies": {
    "@rindo/core": "^2.5.2",
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.3",
    "@types/listr": "^0.14.2",
    "@types/node": "^14.0.14",
    "@types/svgo": "^1.3.3",
    "conventional-changelog-cli": "^2.0.34",
    "execa": "^2.0.4",
    "fs-extra": "9.0.1",
    "inquirer": "^7.2.0",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "listr": "^0.14.3",
    "svgo": "1.3.2",
    "typescript": "3.9.5"
  },
  "keywords": [
    "icon pack",
    "family",
    "icon",
    "svg",
    "mobile",
    "web component",
    "component",
    "custom element",
    "material design",
    "ios"
  ],
  "homepage": "https://famicons.web.app/",
  "author": "Family Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/familyjs/famicons.git"
  },
  "bugs": {
    "url": "https://github.com/familyjs/famicons/issues"
  },
  "license": "MIT",
  "sideEffects": [
    "icons/imports/"
  ]
}
